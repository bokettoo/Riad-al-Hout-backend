{
    "version": 2,
    "builds": [
      {
        "src": "main.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "main.py"
      }
    ],
    "env": {
      "DATABASE_URL": "@DATABASE_URL",
      "SECRET_KEY": "@SECRET_KEY",
      "ALGORITHM": "HS256",
      "ACCESS_TOKEN_EXPIRE_MINUTES": "30"
    }
  }